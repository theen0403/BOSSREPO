
@{
    ViewBag.Title = "PAOOListOfProposedFinalization";
}

<script>
    $("#ReviewListOfProposedFinalizationID").addClass("active");
</script>

@*Header*@
<div class="content-header row">
    <div class="col-12">
        <h3 class="content-header-title"><strong>List of Proposed/Reviewed PAOO for Finalization</strong></h3>
    </div>
</div>
@*BODY*@
<div class="content-body">
    <div class="row">
        <div class="content-header-left col-md-12">
            <div class="card box-shadow-2">
                <div class="card-content collapse show">
                    <div class="card-body rounded" style="border: 1px solid; border-color: #9BC69D;">
                        <div class="row mt-0">
                            <div class="col-md-2 col-sm-1 "></div>
                            <div class="col-md-4 col-sm-5">
                                <fieldset class="">
                                    <label>
                                        <input type="radio" name="RadioS" id="radioFirst">
                                        Office Operational Expenses
                                    </label>
                                </fieldset>
                            </div>
                            <div class="col-md-4 col-sm-5">
                                <fieldset class="">
                                    <label>
                                        <input type="radio" name="RadioS" id="radioSecond">
                                        Programs/Projects/Activities
                                    </label>
                                </fieldset>
                            </div>
                        </div>
                        <div class="row mt-1">
                            <div class="col-md-3"></div>
                            <div class="col-md-2 col-sm-4">
                                <div class="form-group">
                                    <label>Budget Year</label>
                                    <select class="form-control">
                                        <option>2018</option>
                                        <option>2019</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-4 col-sm-4">
                                <div class="form-group">
                                    <label>Appropriation Source</label>
                                    <select class="form-control">
                                        <option>Annual Budget 2018</option>
                                        <option></option>
                                    </select>
                                </div>
                            </div>
                        </div>

                        <div class="row m-0">
                            <div class="col-12 mx-0 px-0">
                                <button class="btn btn-btnbackcolor btn-min-width mb-0 pull-right">Approve/Finalize</button>
                            </div>
                            <div class="table-responsive">
                                <table id="table-program" class="table table-striped table-bordered w-100 zero-configuration table-smss-hover t-has-destination" destination="">

                                    <thead class="text-center">
                                        <tr>
                                            <th width="5%"></th>
                                            <th>Department/Office/Responsibility Center</th>
                                            <th width="7%">Office Code</th>
                                            <th>Program/Project/Activity</th>
                                            <th width="15%">Proposed</th>
                                            <th width="15%">LFC Reviewed</th>
                                            <th width="15%">SP Reviewed</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>
                                                <fieldset class="text-center">
                                                    <input type="checkbox" name="radio2">
                                                </fieldset>
                                            </td>
                                            <td>Office of the PROVINCIAL GOVERNOR</td>
                                            <td>1011</td>
                                            <td>Office Operational Expenses</td>
                                            <td class="text-right">37,721,044.97</td>
                                            <td class="text-right">0.00</td>
                                            <td class="text-right">0.00</td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <fieldset class="text-center">
                                                    <input type="checkbox" name="radio2">
                                                </fieldset>
                                            </td>
                                            <td>PROVINCIAL COOPERATIVE AND ENTERPRISE DEVELOPMENT OFFICE</td>
                                            <td>8761</td>
                                            <td>Office Operational Expenses</td>
                                            <td class="text-right">1,884,000.00</td>
                                            <td class="text-right">0.00</td>
                                            <td class="text-right">0.00</td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <fieldset class="text-center">
                                                    <input type="checkbox" name="radio2">
                                                </fieldset>
                                            </td>
                                            <td>Office of the PROVINCIAL AGRICULTURIST</td>
                                            <td>8711</td>
                                            <td>Office Operational Expenses</td>
                                            <td class="text-right">334,000.00</td>
                                            <td class="text-right">0.00</td>
                                            <td class="text-right">0.00</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>