
@{
    ViewBag.Title = "FileBarangay";
}

<script>
    $("#FileMaintenanceBarangayID").addClass("active");
</script>
@*Header*@
<div class="content-header row">
    <div class=" content-header-left col-md-12 mb-0">
        <h3 class="content-header-title"><strong>Barangay</strong></h3>
    </div>
</div>
<div class="row">
    <div class="content-header-left col-md-12">
        <div class="card box-shadow-4">
            <div class="card-content collapse show">
                <div class="card-body rounded" style="border: 1px solid; border-color: #9BC69D;">
                    @*NAV TAB*@
                    <div class="card box-shadow-2">
                        <div class="row m-0">
                            <ul class="nav nav-tabs nav-underline">
                                <li class="nav-item ">
                                    <a class="nav-link active bg-backcolorheader btn-min-width" id="BrgyNameNaviID" data-toggle="tab" aria-controls="tabBrgyNameID" href="#tabBrgyNameID" aria-expanded="false">Barangay Name</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link bg-backcolorheader btn-min-width" id="BrgyCollectorNaviID" data-toggle="tab" aria-controls="tabBrgyCollectorID" href="#tabBrgyCollectorID" aria-expanded="false">Barangay Collector</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link bg-backcolorheader btn-min-width" id="BrgyBankAccntNaviID" data-toggle="tab" aria-controls="tabBrgyBankAccntID" href="#tabBrgyBankAccntID" aria-expanded="false">Barangay Bank Account</a>
                                </li>
                            </ul>
                            @*TAB*@
                            <div class="tab-content w-100" id="nav-tabContent">
                                <div class="tab-content px-1 pt-1 card-body rounded box-shadow-3">
                                    <div class="tab-pane" id="tabBrgyNameID">
                                    </div>
                                    <div class="tab-pane" id="tabBrgyCollectorID">
                                    </div>
                                    <div class="tab-pane" id="tabBrgyBankAccntID">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script>
    $(function () {
        $("#BrgyNameNaviID").click();
    });
</script>