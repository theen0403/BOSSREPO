@model BOSS.Models.FMAccounts.SubAccountsModel
@{
    BOSSEFConnectionString BOSSDB = new BOSSEFConnectionString();
}
@using BOSS.Models;


<div class="form-group">
    <label style="margin-bottom:2px;">General Account</label>
    @Html.DropDownListFor(m => m.GenAccountID, Model.GeneralAccntList, new { @class = "form-control select2", id = "GenAccountID",
    onchange = "LoadGetSubAccountDT();GetGenAccntCodeField();"
})
</div>

<script>
    $(function () {
        $("#GenAccountID").select2();
    })

    $(function () {
        LoadGetSubAccountDT();
    })
    function LoadGetSubAccountDT() {
        GetSubAccountDT();
    }
</script>