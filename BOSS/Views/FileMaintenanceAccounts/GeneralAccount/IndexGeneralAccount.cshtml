
<div class="row">
    <div class="col-xl-2 col-lg-3 col-md-3">
        <div class="form-group">
            <label class="custom-form-margin">Revision Year</label>
                <select class="form-control">
                    <option>2015</option>
                </select>
        </div>
    </div>
    <div class="col-xl-5 col-lg-5 col-md-5">
        <div class="form-group">
            <label class="custom-form-margin">Allotment Class</label>
                <select class="form-control">
                    <option>Maintenance and Other Operating Expenses</option>
                    <option>Personal Services</option>
                    <option>Capital Outlay</option>
                    <option>Other Operating Expenses</option>
                </select>
        </div>
    </div>
    <div class="col-xl-5 col-lg-4 col-md-4">
        <div class="form-group">
            <label class="custom-form-margin">Accounts</label>
                <select class="form-control">
                    <option>Asset</option>
                </select>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-xl-6 col-lg-6 col-md-6">
        <div class="form-group">
            <label class="custom-form-margin">Major Account Group</label>
                <select class="form-control">
                    <option>Property, Plant and Equipment</option>
                    <option>Financial Liabilities</option>
                    <option>Government Equity</option>
                    <option>Tax Revenue</option>
                    <option>Personnel Services</option>
                </select>
        </div>
    </div>
    <div class="col-xl-6 col-lg-6 col-md-6">
        <div class="form-group">
            <label class="custom-form-margin">Sub Major Account Group</label>
                <select class="form-control">
                    <option>Tax Revenue - Individual and Corporation</option>
                    <option>Land</option>
                    <option>Payables</option>
                    <option>Government Equity</option>
                    <option>Salaries and Wages</option>
                </select>
        </div>
    </div>
</div>
<div class="row skin skin-square">
    <div class="col-xl-3 col-lg-3 col-md-3">
        <div class="form-group">
            <label class="custom-form-margin">General Ledger Account / Contra Account / Sub Account</label>
            <input type="text" class="form-control" value="Land" />
        </div>
    </div>
    <div class="col-xl-1 col-lg-2 col-md-2 px-0 mx-xl-2 text-center">
        <div class="form-group">
            <label class="custom-form-margin mt-xl-3 mt-lg-3 mt-md-3">Miscellaneous Account?</label>
            <fieldset>
                <input type="checkbox" class="form-control icheckbox_square-red" />
            </fieldset>
        </div>
    </div>
    <div class="col-xl-1 col-lg-2 col-md-2 px-0 mx-xl-1 text-center">
        <div class="form-group">
            <label class="custom-form-margin mt-xl-3 mt-lg-3 mt-md-3">Contra Account?</label>
            <fieldset>
                <input type="checkbox" class="form-control icheckbox_square-red" />
            </fieldset>
        </div>
    </div>
    <div class="col-xl-1 col-lg-2 col-md-2 px-0 text-center">
        <div class="form-group">
            <label class="custom-form-margin mt-xl-3 mt-lg-5 mt-md-5">Sub Account?</label>
            <fieldset class=" skin skin-square">
                <input type="checkbox" class="form-control icheckbox_square-green" />
            </fieldset>
        </div>
    </div>
    <div class="col-xl-3 col-lg-3 col-md-3">
        <div class="form-group">
            <label class="custom-form-margin mt-xl-3 mt-lg-2 mt-md-2">General Ledger Account</label>
            <select class="form-control">
                <option></option>
            </select>
        </div>
    </div>
    <div class="col-xl-2 col-lg-3 col-md-3 col-sm-12">
        <div class="form-group">
            <label class="custom-form-margin mt-xl-3">Account Code</label>
            <div class="input-group mt-0">
                <span class="input-group-addon" id="basic-addon1">1-07-01-</span>
                <input type="text" class="form-control less-padding" />
            </div>
        </div>
    </div>
    <div class="col-xl-2 col-lg-3 col-md-3">
        <div class="form-group">
            <label class="custom-form-margin mt-xl-2">With Reserve?</label>
            <select class="form-control">
                <option>No</option>
            </select>
        </div>
    </div>
    <div class="col-xl-2 col-lg-3 col-md-3">
        <div class="form-group">
            <label class="custom-form-margin mt-xl-2">Reserve %?</label>
            <input type="text" class="form-control" disabled />
        </div>
    </div>
    <div class="col-xl-2 col-lg-3 col-md-3">
        <div class="form-group">
            <label class="custom-form-margin mt-xl-2">Full Release?</label>
            <select class="form-control">
                <option>No</option>
            </select>
        </div>
    </div>
    <div class="col-xl-2 col-lg-3 col-md-3">
        <div class="form-group">
            <label class="custom-form-margin mt-xl-2 mt-lg-2 mt-md-2">For Continuing?</label>
            <select class="form-control">
                <option>No</option>
            </select>
        </div>
    </div>
    <div class="col-xl-2 col-lg-3 col-md-3">
        <div class="form-group">
            <label class="custom-form-margin">OBR Cash Advance?</label>
            <select class="form-control">
                <option>No</option>
            </select>
        </div>
    </div>
    <div class="col-xl-2 col-lg-3 col-md-3">
        <div class="form-group">
            <label class="custom-form-margin mt-xl-2 mt-lg-2 mt-md-2">Normal Balance</label>
            <select class="form-control">
                <option>Debit</option>
                <option>Credit</option>
            </select>
        </div>
    </div>
</div>
<div class="row mb-2">
    <div class="col">
        <button type="button" class="btn btn-menuRed btn-min-width float-right ml-1" onclick="GetRevisionCOADT()">
            Cancel
        </button>
        <button type="submit" class="btn btn-btnbackcolor btn-min-width float-right">
            Add
        </button>
    </div>
</div>
